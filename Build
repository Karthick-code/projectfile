pipeline {
    agent any
    Parameters{
        Fileparameter filelocation 'newtext.txt'
    
    }

    stages {
        stage('Build') {
            steps {
                echo 'Build Success'
            }
        }
        stage("upload") {
            steps{
                echo 'upload success' 
                WithFileParameter ('newtext.txt')
                sh 'cat $newtext.txt'
            }
        }
    }
}
